{
  "name": "auth0",
  "version": "4.0.0-beta.0",
  "description": "SDK for Auth0 API v2",
  "main": "dist/index.js",
  "types": "dist/typings/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/typings/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --options typedoc.cjs",
    "test": "npm run test:mocha && npm run test:jest",
    "test:mocha": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' mocha",
    "test:jest": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest",
    "test:ci": "c8 npm run test",
    "precommit": "pretty-quick --staged",
    "lint": "eslint ./src --ext ts",
    "start:playground": "node --experimental-specifier-resolution=node --no-warnings --loader ts-node/esm examples/playground/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auth0/node-auth0"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "auth0",
    "api"
  ],
  "author": "Auth0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/node-auth0/issues"
  },
  "homepage": "https://github.com/auth0/node-auth0",
  "dependencies": {
    "jose": "^4.13.2",
    "node-fetch": "^3.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/jest": "^29.5.0",
    "@types/mocha": "^10.0.1",
    "@types/node-fetch": "^2.6.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "c8": "^7.13.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^3.0.1",
    "jest": "^29.5.0",
    "mocha": "^9.1.3",
    "nock": "^13.2.7",
    "prettier": "^2.8.7",
    "pretty-quick": "^1.11.1",
    "sinon": "^14.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.6",
    "typescript": "4.9.5"
  }
}
