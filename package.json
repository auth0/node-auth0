  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/node-auth0/issues"
  },
  "homepage": "https://github.com/auth0/node-auth0",
  "dependencies": {
    "bluebird": "^2.10.2",
    "rest-facade": "^1.2.3",
    "url-join": "0.0.1"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "mocha": "^2.2.4",
    "nock": "^3.1.1",
    "sinon": "^1.17.1",
    "superagent": "^1.1.0"
  }
}
